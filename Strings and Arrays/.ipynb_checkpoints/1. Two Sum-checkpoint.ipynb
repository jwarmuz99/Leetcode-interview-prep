   "outputs": [],
   "source": [
    "class Solution:\n",
    "    def twoSum(self, nums, target):\n",
    "        hmap = {}\n",
    "        for i in range(len(nums)):\n",
    "            if nums[i] in hmap:\n",
    "                return [hmap[nums[i]], i]\n",
    "            else:\n",
    "                hmap[target-nums[i]] = i"
   ]
